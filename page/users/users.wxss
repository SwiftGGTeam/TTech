.scrollview {
    height: 100%; 
    text-align: center; 
    padding-bottom: 5px; 
    padding-top: 1rem; 
}
.view_cell {
    padding: 0.5rem 0.7rem; 
    display: flex; 
    flex-wrap: wrap; 
    justify-content: space-between; 
}
.navigator_cell {
    width: 8.9rem; 
    height: 8.9rem; 
    background-color: rgb(237, 237, 237); 
    margin-bottom: 0.7rem; 
    position: relative; 
}
.image_cell {
    width: 100%; 
    height: 100%; 
}
.view_cell_bottom {
    position: absolute; 
    width: 100%; 
    bottom: 0px; 
    background-color: rgba(0, 0, 0, 0.811765); 
    display: flex; 
    justify-content: center; 
    height: 30px; 
    align-items: center; 
    padding-left: 10px; 
    padding-right: 10px; 
}
.text_cell_bottom {
    font-size: 12px; 
    color: #dedede; 
}