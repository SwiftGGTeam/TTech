.text_title {
    display: flex; 
    justify-content: center; 
    align-items: center; 
    font-size: 25px; 
    margin-top: 15px; 
    margin-bottom: 15px; 
}
.view_input {
    display: flex; 
    flex-direction: column; 
    align-items: center; 
    justify-content: center; 
    min-height: 50px; 
}
.navigator_input {
    padding: 8px;
    border-width: 1px;
    text-align: center;
    color: rgb(0, 133, 255);
    border-color: rgb(0, 133, 255);
    font-size: 16px;
    width: 75%;
    align-self: center;
    border-bottom-style: solid;
    border-radius: 0px;
  }